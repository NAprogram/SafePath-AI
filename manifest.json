{
    "manifest_version": 3,
    "name": "SafePath AI",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "48": "logo.png",
        "128": "logo.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://localhost:5000/*"
    ]
}